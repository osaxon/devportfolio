    "@react-spring/parallax": "^9.6.0",
    "@react-spring/web": "^9.6.0",
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-query-devtools": "^4.20.4",
    "axios": "^0.27.2",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.2.1",
    "daisyui": "^2.42.1",
    "date-fns": "^2.29.3",
    "firebase": "^9.10.0",
    "framer-motion": "^7.3.6",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "lodash": "^4.17.21",
